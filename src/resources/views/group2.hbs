	<div id="right-menu">
		<div id="task-container">
			<fieldset id="create-task">
				<legend>Create task:</legend>
				<div id="task-box">
					<input type="text" placeholder="Type any task here" />
					<button id="create-btn">Create</button>
				</div>
				<p id="is-empty"><small>Empty input!</small></p>
				<p id="is-created"><small>Task has already been created!</small></p>
			</fieldset>
		</div>
	</div>
	<div id="footer">
		<fieldset id="custom">
			<legend>Customization:</legend>
			<div class="box-custom">
				<button type="submit" id="save">Save</button>
			</div>
			<div class="box-custom">
				<input type="color" id="preset" />
				<button id="default">Default color</button>
				<button id="reset">Reset all</button>
			</div>
			<div class="box-custom">
				<button id="hide-create">Hide/show create menu</button>
				<button id="down-btn">Save as png</button>
			</div>
		</fieldset>
		<button id="undo">Undo</button>
		<button id="redo">Redo</button>
	</div>
	<div id="left">
		<button id="btn-1">Group 1</button>
		<button id="btn-2">Group 2</button>
	</div>
