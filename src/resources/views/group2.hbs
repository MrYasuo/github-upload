						class="task-each"
						onclick="toggleTask()"
						style="background-color: {{background}}; color:{{color}}"
					>{{task}}
						<ul></ul>
						<img
							src="times-solid.svg"
							id="delete-data"
							onclick="deleteData()"
						/>
					</td>
